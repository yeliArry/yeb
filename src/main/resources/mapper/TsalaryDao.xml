<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hdax.yeb.dao.TsalaryDao">
    <update id="upd" parameterType="int">
        update t_salary
        set allSalary=#{allSalary}
        where id = #{id};
    </update>
    <update id="updbasicSalary" parameterType="int">
        update t_salary
        set basicSalary = #{basicSalary}
        where id = #{id}
    </update>

    <select id="getMoneyByEid" resultType="com.hdax.yeb.entity.Tsalary" parameterType="int">

    </select>
</mapper>